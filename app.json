    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e7e6a95d-3b82-4fab-a2d7-f9b12290c1e0"
      }
    },
    "android": {
      "package": "com.mmulobi.boltexponativewind"
